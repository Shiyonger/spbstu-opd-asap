{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "PostgresConnectionString": "User ID=postgres;Password=pwd;Host=localhost;Port=5431;Database=asap;Pooling=true;",
