    "Action": {
      "BootstrapServers": "localhost:9092",
      "GroupId": "2",
      "Topic": "action",
      "ChannelCapacity": 1000,
      "BufferDelaySeconds": 0
    }
  },
  "KafkaPublisherOptions": {
    "Points": {
      "BootstrapServers": "localhost:9092",
      "Topic": "points-google",
      "BatchSize": 50000,
      "LingerMs": 5,
      "ChunkSize": 100
    },
    "Queue": {
      "BootstrapServers": "localhost:9092",
      "Topic": "queue",
      "BatchSize": 50000,
      "LingerMs": 5,
      "ChunkSize": 100
    }
